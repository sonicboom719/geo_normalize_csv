{
	"input": "./sample/中央区.csv",
	"output": "./sample/中央区_normalized.csv",
	"api": {
		"key": "YOUR_GOOGLE_API_KEY",
		"sleep": 200,
		"mode": "reverse_geocode",
		"reverse_geocode_check": true,
		"gsi_check": {
			"check": true,
			"distance": 200,
			"priority": "gsi"
		}
	},
	"format": {
		"prefecture": "東京都",
		"city": "中央区",
		"number": "{2}",
		"address": "{3}",
		"name": "{4}",
		"lat": "{lat}",
		"long": "{long}",
    	"note": ""
	}
}
